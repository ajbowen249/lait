{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "test",
          "test:unit",
          "build-only",
          "type-check",
          "lint"
        ]
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "{projectRoot}/dist"
      ]
    },
    "test": {
      "dependsOn": [
        "^test"
      ]
    },
    "test:unit": {
      "dependsOn": [
        "^test:unit"
      ]
    },
    "build-only": {
      "dependsOn": [
        "^build-only"
      ],
      "outputs": [
        "{projectRoot}/dist"
      ]
    },
    "type-check": {
      "dependsOn": [
        "^type-check"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    }
  }
}
